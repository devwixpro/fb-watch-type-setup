{
    "name": "TeamX Looper",
    "description": "try to auto loop",
    "version": "2.2.0.0",

    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },

    "browser_action": {
        "default_title": "Video Looper",
        "default_icon": {
            "19": "icon-19.png",
            "38": "icon-38.png"
        }
    },

    "permissions": [
        "activeTab",
        "<all_urls>",
		"browsingData"
    ],

    "icons": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
    },
    "manifest_version": 2
}
